    "list.inactiveFocusBackground": "{{ ui.accent }}{{ alpha.med }}",
    "list.errorForeground": "{{ ui.negative }}{{ alpha.high }}",
    "list.warningForeground": "{{ ui.warning }}{{ alpha.highMed }}",
    "activityBar.background": "{{ ui.background }}",
