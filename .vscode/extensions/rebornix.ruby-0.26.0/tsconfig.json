{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowJs": true,
		"noImplicitReturns": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"rootDir": "src",
		"outDir": "dist"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
